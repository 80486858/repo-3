{
  "name": "discord-soundbot",
  "version": "1.6.0-dev",
  "description": "A Soundboard for Discord",
  "main": "dist/src/index.js",
  "bin": {
    "soundbot": "dist/bin/soundbot.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markokajzer/discord-soundbot"
  },
  "keywords": [
    "discord",
    "bot",
    "soundboard",
    "voice",
    "sounds",
    "mp3",
    "nodejs",
    "typescript"
  ],
  "author": "Marko Kajzer",
  "license": "MIT",
  "dependencies": {
    "awilix": "^4.2.2",
    "discord.js": "11.5.1",
    "fluent-ffmpeg": "^2.1.2",
    "i18n": "^0.8.3",
    "lowdb": "^1.0.0",
    "node-opus": "^0.2.7",
    "ytdl-core": "^0.29.1"
  },
  "optionalDependencies": {
    "bufferutil": "^3.0.5",
    "erlpack": "discordapp/erlpack",
    "libsodium-wrappers": "^0.7.4"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "2.1.2",
    "@types/glob": "7.1.1",
    "@types/i18n": "0.8.5",
    "@types/lowdb": "1.0.7",
    "@types/node": "10.14.6",
    "replace-in-file": "^4.0.0",
    "tslint": "^5.16.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc -p tsconfig.json",
    "build": "npm run compile",
    "lint": "tslint -p tsconfig.json",
    "postcompile": "node dist/lib/postbuild.js",
    "serve": "node dist/bin/soundbot.js",
    "start": "npm run build && npm run serve",
    "rebuild": "npm run clean && npm run build",
    "release": "npm run rebuild && npm run lint"
  }
}
